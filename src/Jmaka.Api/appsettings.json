{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Jmaka.FFmpeg": "Information"
    }
  },
  "AllowedHosts": "*",
  "Ffmpeg": {
    "DefaultEncodingMode": "BALANCED",
    "BalancedDurationLimitSeconds": 120,
    "EnableRamBasedFallback": true,
    "MinAvailableRamMbFor720p2Pass": 1200,
    "MinAvailableRamMbFor720pAny": 800,
    "RamSafetyMarginMb": 200,
    "MaxConcurrentFfmpegJobs": 1,
    "FfmpegQueueMaxLength": 200,
    "FfmpegQueueJobTtlMinutes": 180,
    "FfmpegQueuePollIntervalMs": 200,
    "FfmpegQueueOnOverflow": "REJECT_429"
  }
}
